datasource db {
  url      = env("DB_URL")
  provider = "postgresql"
}

generator client {
  provider = "prisma-client-js"
}

model City {
  id    String @id @default(uuid())
  name  String
  state String

  User User[]
  @@map("cities")
}

model User {
  id        String   @id @default(uuid())
  fullname  String   @unique
  sex       String
  birthdate DateTime
  age       Int
  city      City     @relation(fields: [cityId], references: [id])
  cityId    String

  @@map("users")
}
